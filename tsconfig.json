{
  "compilerOptions": {
    "lib": ["ES2021", "DOM"],
    "module": "ESNext",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": true,
    "pretty": true,
    "declaration": true,
    "types": [],
    "typeRoots": [
      "node_modules/@types"
    ],
  },
  "include": ["src"]
}


